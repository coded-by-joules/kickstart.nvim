  "MAC_sectiontime": {
    "prefix": "MAC_sectiontime",
    "body": [
      "<float ",
      "  label=\"x_$1_DURATION_start\"",
      "  size=\"20\"",
      "  translateable=\"0\"",
      "  where=\"none\">",
      "  <title>Total time start</title>",
      "</float>",
      "",
      "<suspend/>",
      "<exec>",
      "x_$1_DURATION_start.val = timeSpent()",
      "</exec>",
      "",
      "$3",
      "",
      "<number ",
      "  label=\"x_$1_DURATION\"",
      "  size=\"20\"",
      "  translateable=\"0\"",
      "  where=\"execute,survey,report\">",
      "  <title>Total time spent in $2</title>",
      "  <exec>",
      "x_$1_DURATION.val  = int(round((timeSpent() - x_$1_DURATION_start.ival),0))",
      "  </exec>",
      "",
      "</number>",
      "",
      "<suspend/>"
    ]
  }
