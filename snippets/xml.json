  "flash_quote": {
    "prefix": "flash_quote",
    "body": [
      "import urllib",
      "",
      "def flash_quote(string):",
      "    \"\"\"",
      "    Checks if flashDetected is True,",
      "    If True, returns URL escaped string",
      "    Else returns string unchanged",
      "    \"\"\"",
